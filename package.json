{
  "name": "skillshare-complete",
  "version": "1.0.0",
  "description": "Complete SkillShare PWA with all functionality",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "start-backend": "cd backend && node src/working-server.js",
    "start-frontend": "cd frontend && npm run dev",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "dev": "npm run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "mongoose": "^8.19.1",
    "multer": "^2.0.2"
  }
}
